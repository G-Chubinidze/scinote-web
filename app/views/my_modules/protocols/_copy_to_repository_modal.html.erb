<div class="modal" id="copy-to-repository-modal" tabindex="-1" role="dialog" aria-labelledby="copy-to-repository-modal-label">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span title="close copy to repository modal" aria-hidden="true">&times;</span></button>
        <h2 class="modal-title">
          <%= t("my_modules.protocols.copy_to_repository_modal.title") %>
        </h2>
      </div>
      <div class="modal-body"></div>
      <div class="modal-footer">
        <button title="cancel copying to repository" type="button" class="btn btn-secondary" data-dismiss="modal"><%= t("general.cancel") %></button>
        <button title="confirm copy to repository" type="button" class="btn btn-primary" data-action="submit"><%= t("my_modules.protocols.copy_to_repository_modal.confirm") %></button>
      </div>
    </div>
  </div>
</div>
